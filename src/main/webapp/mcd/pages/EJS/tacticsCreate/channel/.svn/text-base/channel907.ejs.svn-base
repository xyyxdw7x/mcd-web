<div class="channel-app-title J_show_box hidden" channelId="<%=data.channelId%>" >
    <span class="app-title fleft"><%=data.channelName%></span>
    <img class="preview-image previewExeContent-907" channelId="<%=data.channelId%>" src="../../assets/images/preview-all.png">
	<span class="preview-span previewExeContent-907" channelId="<%=data.channelId%>">效果预览</span>
</div>
<div class="channel-operation-box J_show_box hidden" channelId="<%=data.channelId%>" >
    <div class="fleft channel-left-list">
        <ul class="channel-operation-box"  channelid="<%=data.channelId%>">
            <li class="channel-operation-box clearfix">
                <div class="fleft channel-operation-left">选择运营位
                </div>
                <ul class="channel-selected-img-ul" channelid="<%=data.channelId%>">
                    <li class="channel-selected-img unselect fleft toolbar-adiv-1" adivsize="100" adivid="1"
                        adivname="首页推荐" adivdesc="首页推荐" chnresourceid="" channelid="<%=data.channelId%>"
                        chnresourcedesc="" adivcontenttourl=""
                        adivcontenturl="首页推荐" adivresourcename="首页推荐！">
                    </li>
                    <li class="channel-selected-img unselect fleft toolbar-adiv-2" adivsize="100" adivid="2"
                        adivname="弹窗" adivdesc="弹窗" chnresourceid="" channelid="<%=data.channelId%>"
                        chnresourcedesc="" adivcontenttourl=""
                        adivcontenturl="弹窗" adivresourcename="弹窗！">
                    </li>
                </ul>
            </li>
            <li class="channel-operation-box clearfix">
                <div class="fleft channel-operation-left">推荐用语
                </div>
                <div class="fleft channel-operation-main">
                    <div class="channel-operation-main-inner clearfix">
                        <div class="channel-textarea-box fleft">
                            <textarea rows="" cols="" class="channel-textarea J_SMSBox"  channelId="<%=data.channelId%>" maxTextNum="<%=data.wordSize%>"></textarea>
                            <p class="channel-num-text">您还可以输入<i class="yellow" ><%=data.wordSize%></i>个字</p>
                        </div>
                        <div class="preview-message fleft hidden">
                            <button type="button" class="preview-btn previewExeContent-907" channelId="<%=data.channelId%>">预览效果</button>
                        </div>
                    </div>
                </div>
            </li>
        </ul>

    </div>
</div>
