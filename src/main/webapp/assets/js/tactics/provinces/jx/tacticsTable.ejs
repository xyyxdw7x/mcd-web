<table class="content-table">
	<tr>
		<th class="content-table-th" style="width:6%">序号</th>
		<th class="content-table-th" style="width:44%">策略编号及名称</th>
		<th class="content-table-th" style="width:15%">执行周期</th>
		<th class="content-table-th" style="width:7%">状态</th>
		<th class="content-table-th" style="width:18%">操作</th>
	</tr>
	<%for(var i = 0;i<data.result.length;i++){
	   data.result[i].DQ_D_2_NAME="距离营销策略开始日期";
	   if(parseInt(data.result[i].DQ_D_2)<=0){
	   	 data.result[i].DQ_D_2=Math.abs(parseInt(data.result[i].DQ_D_2));
	   	 data.result[i].DQ_D_2_NAME="已超期";
	   }
	   //如果任务状态为30 则策略状态为 数据准备中
	   if(data.result[i].taskStatusId=="30"){
	   	  data.result[i].campsegStatName="数据准备中";
	   }
	%>
		<tr class="content-table-tr <%if(i%2==1){%>odd<%}else{%>even<%}%>"> 
			<td class="content-table-td"> 
			<%if(data.result[i].DQ_1=='1'){%> 
				<span  value="<%=data.result[i].DQ_D_1%>"><img src=""/></a><img src="../../assets/images/dqcl.png"   alt="【策略到期提醒】距离营销策略到期日期<%=data.result[i].DQ_D_1%>天" title="【策略到期提醒】距离营销策略到期日期<%=data.result[i].DQ_D_1%>天"/></span>
            <%} else  {  if(data.result[i].DQ_2=='1') {%> 
		    <span  value="<%=data.result[i].DQ_D_2%>"> <img src="../../assets/images/dqsp.png" alt="【审批到期提醒】<%=data.result[i].DQ_D_2_NAME%><%=data.result[i].DQ_D_2%>天" title="【审批到期提醒】<%=data.result[i].DQ_D_2_NAME%><%=data.result[i].DQ_D_2%>天" /></span>
		        <%} else { if(data.result[i].DQ_3=='1') {%> 
				  <span  value="<%=data.result[i].DQ_D_3%>"><img src="../../assets/images/dqzc.png" alt="【政策到期提醒】距离营销政策到期日期<%=data.result[i].DQ_D_3%>天"  title="【政策到期提醒】距离营销政策到期日期<%=data.result[i].DQ_D_3%>天"/></span>
			 <%}}}%>  <%=i+1%>   
			  </td>
			<td class="content-table-td">
				<a href="<%="tacticsInfo.jsp?campsegId="+data.result[i].campsegId%>" target="_blank">
					<%=data.result[i].campsegId%>&nbsp;<%=data.result[i].campsegName%>
				</a>
			</td>
			<td class="content-table-td align_center tactics_manager_act_cycle" campsegId="<%=data.result[i].campsegId%>" invalidFlag="<%=data.result[i].invalidFlag%>"  endDate="<%=data.result[i].endDate%>"><%=data.result[i].startDate%>到<%=data.result[i].endDate%></td>
			<td class="content-table-td <%=data.result[i].campsegStatName=='暂停'?'more_tips':''%>" pauseComment="<%=data.result[i].pauseComment%>" style="overflow: visible;"><%=data.result[i].campsegStatName%></td>
			<td class="content-table-td align_center J_manageTd">
				<%
					  var onlyCpoyArr = ["91","90","59","54","50","40","48"];
					  var statusId = data.result[i].campsegStatId+"";
					  data.result[i].manageBtnDisplay="visible";
					  if($.inArray(statusId, onlyCpoyArr) != -1){
					    if((data.result[i].DQ_2+"")!="1")
						{
						    data.result[i].manageBtnDisplay="hidden";
						} 
					  }
					  if(data.result[i].taskStatusId=="30"){
					  	data.result[i].manageBtnDisplay="hidden";
					  }
					%>
				<div class="fleft controls manage J_manageBtn <%=data.result[i].manageBtnDisplay%>" style="visibility:<%=data.result[i].manageBtnDisplay%>" isMy="<%=data.result[i].isMy%>" <%=(data.result[i].campsegStatId != 90 ) ? '': 'style="display:none;"' %>  statusId="<%=data.result[i].campsegStatId%>" campsegId="<%=data.result[i].campsegId%>" DQ_2="<%=data.result[i].DQ_2%>" >
					<span class="txt J_manageBtn"  isMy="<%=data.result[i].isMy%>" statusId="<%=data.result[i].campsegStatId%>" campsegId="<%=data.result[i].campsegId%>" DQ_2="<%=data.result[i].DQ_2%>" >管理</span>
					<div class="icon J_manageBtn" isMy="<%=data.result[i].isMy%>" statusId="<%=data.result[i].campsegStatId%>" invalidFlag="<%=data.result[i].invalidFlag%>" campsegId="<%=data.result[i].campsegId%>" DQ_2="<%=data.result[i].DQ_2%>" >
						<span class="icon_arrDown J_manageBtn" statusId="<%=data.result[i].campsegStatId%>" isMy="<%=data.result[i].isMy%>" campsegId="<%=data.result[i].campsegId%>" DQ_2="<%=data.result[i].DQ_2%>" ></span>
					</div>
				</div>
				<div class="fleft controls manage J_activityBtn" <%=(data.result[i].campsegStatId == 20 || data.result[i].campsegStatId == 90|| data.result[i].campsegStatId == 91|| data.result[i].campsegStatId == 41|| data.result[i].campsegStatId == 48||data.result[i].taskStatusId==30) ?  'style="display:none;"': ''%> isMy="<%=data.result[i].isMy%>" statusId="<%=data.result[i].campsegStatId%>" campsegId="<%=data.result[i].campsegId%>">
					<span class="txt J_activityBtn"  isMy="<%=data.result[i].isMy%>" statusId="<%=data.result[i].campsegStatId%>" campsegId="<%=data.result[i].campsegId%>">活动管控</span>
					<div class="icon J_activityBtn" isMy="<%=data.result[i].isMy%>" statusId="<%=data.result[i].campsegStatId%>" campsegId="<%=data.result[i].campsegId%>">
						<span class="icon_arrDown J_activityBtn" statusId="<%=data.result[i].campsegStatId%>" isMy="<%=data.result[i].isMy%>" campsegId="<%=data.result[i].campsegId%>"></span>
					</div>
				</div>
			</td>
		</tr>
		<tr>
			<td colspan="5" class="nopadding">
				<div class="tableOuter">
					
				</div>
			</td>
		</tr>
	<%}%>
</table>
<div class="content-table-page">
	<div class="fright clearfix centent-page-box">
		<%if(!data.hasPrevPage || data.totalSize == 0){%>
			<a href="javascript:;" class="page-button fleft disabled">上一页</a>
		<%}else{%>
			<a href="javascript:;" class="page-button fleft" data-flag="prev">上一页</a>
		<%}%>
		<%if(data.totalPage>5){%>
			<%if(data.pageNum<5 || data.pageNum>data.totalPage-2){%>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==1){%>active<%}%>">1</a>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==2){%>active<%}%>">2</a>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==3){%>active<%}%>">3</a>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==4){%>active<%}%>">4</a>
				<a href="javascript:;" class="page-num fleft" data-flag="last-dot" style="pointer-events:none;">···</a>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==data.totalPage-1){%>active<%}%>" ><%=data.totalPage-1%></a>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==data.totalPage){%>active<%}%>"><%=data.totalPage%></a>
			<%}else if(data.pageNum>4 && data.pageNum<data.totalPage-1){%>
				<a href="javascript:;" class="page-num fleft ">1</a>
				<a href="javascript:;" class="page-num fleft">2</a>
				<%if(data.totalPage-2 == data.pageNum){%>
					<a href="javascript:;" class="page-num fleft">3</a>
				<%}%>
				<a href="javascript:;" class="page-num fleft" data-flag="first-dot" style="pointer-events:none;">···</a>
				<a href="javascript:;" class="page-num fleft active"><%=data.pageNum%></a>
				<%if(data.totalPage-2 != data.pageNum){%>
					<a href="javascript:;" class="page-num fleft" data-flag="last-dot" style="pointer-events:none;">···</a>
				<%}%>
				<a href="javascript:;" class="page-num fleft"><%=data.totalPage-1%></a>
				<a href="javascript:;" class="page-num fleft"><%=data.totalPage%></a>
			<%}%>
		<%}else{%>
			<%for(var i =1; i<=data.totalPage;i++){%>
				<a href="javascript:;" class="page-num fleft <%if(data.pageNum==i){%>active<%}%>"><%=i%></a>
			<%}%>
		<%}%>
		<%if(!data.hasNextPage || data.totalSize == 0){%>
			<a href="javascript:;" class="page-button fleft disabled">下一页</a>
		<%}else{%>
			<a href="javascript:;" class="page-button fleft" data-flag="next">下一页</a>
		<%}%>
		<span class="fleft">
			<input type="text"class="ui-page-num"/>
		</span>
		<a href="javascript:;" class="ui-page-button fleft">跳转</a>
	</div>
</div>

