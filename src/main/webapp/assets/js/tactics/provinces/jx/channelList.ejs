<%if(data.length > 0){%>
	<%for(var i=0;i<data.length;i++){
		var item=data[i];
	%>
	<li class="content-channel-box fleft J_channelBox" channelId="<%=item.channelId%>" id="li_channelId_<%=item.channelId%>">
		<span><img class="pics" src="<%=contextPath%>/mcd/assets/images/<%=item.channelId%>.png"></span>
		<span class="type-title my-channel-name"><%=item.channelName%></span>
		<img class="selected-img my-selected-icon" src="<%=contextPath%>/mcd/assets/images/channelSelected.png" style="display:none;">
	</li>
	<%}%>
<%}%>