<table class="content-table">
    <tr>
        <th class="content-table-th" style="width:5%">序号</th>
        <th class="content-table-th" style="width:10%">客户群编号</th>
        <th class="content-table-th" style="width:15%">客户群名称</th>
        <th class="content-table-th" style="width:5%">创建人</th>
        <th class="content-table-th" style="width:5%">更新周期</th>
        <th class="content-table-th" style="width:10%">当前数据日期</th>
        <th class="content-table-th" style="width:10%">数据延迟日期</th>
        <th class="content-table-th" style="width:10%">数据异常原因</th>
        <th class="content-table-th" style="width:10%">操作</th>
    </tr>
    <%
    if(data.result.length==0){%>
    	 <tr class="content-table-tr ">
    	  	<td class="content-table-td" colspan="9">暂无数据</td>
    	 </tr>
    <%}%>
    <%for(var i = 0;i<data.result.length;i++){%>
        <tr class="content-table-tr <%if(i%2==1){%>odd<%}else{%>even<%}%>">
            <td class="content-table-td"><%=i+1%></td>
            <td class="content-table-td"><%=data.result[i].CUSTOM_GROUP_ID%></td>
            <td class="content-table-td td-align-left"><%=data.result[i].CUSTOM_GROUP_NAME%></td>
            <td class="content-table-td"><%=data.result[i].CREATE_USER_NAME%></td>
            <td class="content-table-td"><%=data.result[i].UPDATE_CYCLE_NAME%></td>
            <td class="content-table-td orangeFont"><%=data.result[i].DATA_TIME_STR%></td>
            <td class="content-table-td orangeFont"><%=data.result[i].DELAY_DAYS%>天</td>
            <td class="content-table-td orangeFont"><%=data.result[i].exceptionReason%></td>
            <td class="content-table-td">
                <div class="operate-btn-container">
                    <div class="look-up-custom">查看</div>
                    <%if(data.result[i].DEL_FLAG=='1'){%>
                        <div class="delete-custom-enabled">删除</div>
                    <%}else{%>
                        <div class="delete-custom-disabled">删除</div>
                    <%}%>
                </div>
            </td>
        </tr>
    <%}%>
</table>
<div class="content-table-page">
    <div class="fright clearfix centent-page-box">
    </div>
</div>

