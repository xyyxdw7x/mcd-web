<table class="content-table">
    <tr>
        <th class="content-table-th" style="width:3%">序号</th>
        <th class="content-table-th" style="width:10%">客户群编号</th>
        <th class="content-table-th" style="width:15%">客户群名称</th>
        <th class="content-table-th" style="width:10%">创建人</th>
        <th class="content-table-th" style="width:7%">更新周期</th>
        <th class="content-table-th" style="width:10%">状态</th>
        <th class="content-table-th" style="width:10%">客户群规模</th>
        <th class="content-table-th" style="width:10%">数据日期</th>
        <th class="content-table-th" style="width:15%">操作</th>
    </tr>
    <%if(data.result.length==0){%>
    	 <tr class="content-table-tr ">
    	  	<td class="content-table-td" colspan="9">暂无数据</td>
    	 </tr>
    <%}%>
    <%for(var i = 0;i<data.result.length;i++){%>
        <tr class="content-table-tr <%if(i%2==1){%>odd<%}else{%>even<%}%>" data-table-name="<%=data.result[i].LIST_TABLE_NAME%>">
            <td class="content-table-td"><%=i+1%></td>
            <td class="content-table-td"><%=data.result[i].CUSTOM_GROUP_ID%></td>
            <td class="content-table-td td-align-left" title="<%=data.result[i].CUSTOM_GROUP_NAME%>"><%=data.result[i].CUSTOM_GROUP_NAME.length>14?data.result[i].CUSTOM_GROUP_NAME.substring(0,14)+'...':data.result[i].CUSTOM_GROUP_NAME%></td>
            <td class="content-table-td"><%=data.result[i].CREATE_USER_NAME%></td>
            <td class="content-table-td"><%=data.result[i].UPDATE_CYCLE_NAME%></td>
            <td class="content-table-td orangeFont"><%=data.result[i].CUSTOM_STATUS%></td>
            <td class="content-table-td orangeFont"><%=data.result[i].CUSTOM_NUM%></td>
            <td class="content-table-td orangeFont"><%=data.result[i].DATA_TIME_STR%></td>
            <td class="content-table-td">
                <div class="operate-btn-container" style="width: 170px;">
	                <div class="look-up-custom">查看</div>
	                <%if(data.result[i].DEL_FLAG=='1'){%>
                        <div class="delete-custom-enabled">删除</div>
                    <%}else{%>
                        <div class="delete-custom-disabled">删除</div>
                    <%}%>
                </div>
            </td>
        </tr>
    <%}%>
</table>

<div class="content-table-page" >
<div class="fright clearfix centent-page-box" id="custPageDiv"></div>  
</div>

